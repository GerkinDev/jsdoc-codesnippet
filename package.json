	},
	"keywords": [
		"jsdoc",
		"snippet",
		"description",
		"plugin"
	],
	"author": "Alexandre Germain",
	"license": "Apache v2.0",
	"bugs": {
		"url": "https://github.com/Nihilivin/jsdoc-codesnippet/issues"
	},
	"homepage": "https://github.com/Nihilivin/jsdoc-codesnippet",
	"dependencies": {
		"js-beautify": "^1.6.2"
	}
}
